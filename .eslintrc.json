{
  "extends": [
    "next/core-web-vitals",
    "airbnb",
    "prettier"  
  ],
  "rules": {
    "import/extensions":[
      "error",
      "ignorePackages",
      {
        "ts": "never",
        "tsx": "never"
      }
    ],
    "import/prefer-default-export": "off",
    "react/react-in-jsx-scope": "off",
    "react/jsx-filename-extension": "off"

  }
}
